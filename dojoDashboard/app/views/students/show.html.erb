
<h1><%=@dojo.branch%>'s <%=@student.first_name%> <%=@student.last_name%></h1>

<h3>Email: <%=@student.email %></h3>

<table>
    
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
    
    <% @people.each do |x| %>
        <% if x.id != @student.id %>
        <tr>
            <td><%=x.first_name%></td>
            <td><%=x.last_name%></td>
            <td><%=x.email%></td>
        </tr>
        <% end %>
    <% end %>
    
    </table>


    <p><a href="/">Home</a></p>
    <p><a href="/dojos/<%=@dojo.id%>/students/<%= @student.id %>/edit">Edit</a></p>