<h1>Students#new</h1>
<br>
<br>
<p><form action="/dojos/<%= @dojo.id %>/students" method="post">
<input name="authenticity_token" value="<%= form_authenticity_token %>" type="hidden">

<p><label for="first_name"> First Name: </label>
<input type="text" name="student[first_name]"></p>

<p><label for="last_name"> Last Name: </label>
<input type="text" name="student[last_name]"></p>

<p><label for="email"> Email Address: </label>
<input type="text" name="student[email]"></p>

<p><label for="dojo"> Dojo Campus: </label>
<select name="student[dojo_id]">
 <% @dojos.each do |dojo| %>
    <option value="<%= dojo.id %>"><%= dojo.branch %></option>
<% end %>
</p>
<br>

<p><input type="submit" class="btn btn-default" value="Add Student"></p>
</form>

<p><a href="/">Home</a></p>


