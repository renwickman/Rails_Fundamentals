
<a href="/">Home</a>
<a href="/edit/<%=@dojo.id%>">Edit</a>

<p><h1><%=@dojo.branch%></h1></p>

<p><%=@dojo.street%></p>

<p><%=@dojo.city%></p>

<p><%=@dojo.state%></p>


<h2>Student Roster</h2>
<table>
    <thead>
        <tr>
            <th>First Name: </th>
            <th>Last Name: </th>
            <th>Email: </th>
            <th>Actions: </th>
        </tr>
    </thead>
    <tbody>
    <% @students.each do |student| %>
        <tr>
            <td><%=  student.first_name %></td>
            <td><%=  student.last_name %></td>
            <td><%=  student.email %></td>
            <td>
            <a href="/dojos/<%= @dojo.id %>/students/<%= student.id %>">Show</a> | <a href="/dojos/<%= @dojo.id%>/students/<%= student.id %>/edit">Edit</a> | <a href="/dojos/<%= @dojo.id %>/students/<%= student.id %>" data-method="delete">Delete</a>
            </td>
        </tr>
    <% end %>
    </tbody>
</table>
<hr>

<button class="btn btn-default"><a href="/">Home</a></button>

<button class="btn btn-default"><a href="/dojos/<%= @dojo.id %>/students/new">Add New Student</a></button>

